{% extends "base.djhtml" %}

{% block content %}
	{% with messages = get_flashed_messages() %}
		{% if messages  %}
			<ul>
				{% for message in messages %}
					<li>{{ message }}</li>
				{% endfor %}
			</ul>
		{% endif %}
	{% endwith %}

	<img src="{{ hero.image }}">
	<h1>{{ hero.name }} information</h1>
{% endblock %}
